    "uninstaller": {
        "script": [
            "$compilerDir = scoop prefix autohotkey;",
            "Remove-Item -Path \"$compilerDir\\Compiler\\Ahk2Exe.exe\";",
            "Copy-Item -Path \"$dir\\BACKUP_Ahk2Exe.exe\" -Destination \"$compilerDir\\Compiler\\Ahk2Exe.exe\" -Force;"
        ]
    },
    "depends": "extras/autohotkey"
