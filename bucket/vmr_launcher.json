    "bin":[["VMR_launcher.exe","voicemeeter"]],
    "notes": [
        "USAGE:",
        "voicemeeter \"command\" \"Strip[i]/Bus[i]\" [ \"Audio device type\" \"Substring of an audio device's name\" ]",
        "command: ",
        "\tgain++          : increases gain by 2 dB",
        "\tgain--          : decreases gain by 2 dB",
        "\tgain--          : decreases gain by 2 dB",
        "\tmute            : toggles mute state",
        "\tsetAudioDevice  : ",
        "\tvmEngineRestart : restarts Voicemeeter's engine",
        "Audio device type: ",
        "\t1   : mme",
        "\t3   : wdm",
        "\t4   : ks",
        "\t5   : asio",
        "examples: ",
        "\tvoicemeeter gain++ Bus[0] ",
        "\tvoicemeeter gain-- Strip[3] ",
        "\tvoicemeeter setAudioDevice Bus[0] 3 nvidia "
    ]
}
